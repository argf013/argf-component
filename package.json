{
  "name": "argf-react",
  "version": "2.1.39",
  "private": false,
  "description": "Reusable React components",
  "author": "Muhamad Arfa Alghiffari",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "/dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/argf013/argf-react.git"
  },
  "scripts": {
    "dev": "vite",
    "clean": "rimraf dist",
    "build:css": "npx tailwindcss -i ./src/index.css -o ./style.css",
    "build": "npm run clean && npm run build:css && tsc && vite build && grunt bump",
    "lint": "eslint . --ignore-pattern dist *.cjs",
    "preview": "vite preview",
    "prepublishOnly": "npm run build",
    "getLatestVersion": "node version.cjs",
    "git": "pnpm getLatestVersion && git add ."
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.11.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/react": "^18.3.8",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.6.0",
    "@typescript-eslint/parser": "^8.6.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.11.0",
    "eslint-plugin-react": "^7.36.1",
    "eslint-plugin-react-hooks": "5.1.0-rc-fb9a90fa48-20240614",
    "eslint-plugin-react-refresh": "^0.4.12",
    "grunt": "^1.6.1",
    "grunt-bump": "^0.8.0",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.12",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.6.0",
    "vite": "^5.4.7",
    "vite-plugin-dts": "^4.2.1"
  },
  "dependencies": {
    "@primer/octicons-react": "^19.11.0",
    "clsx": "^2.1.1",
    "eslint-plugin-prettier": "^5.2.1",
    "react-tooltip": "^5.28.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "vite-plugin-css-injected-by-js": "^3.5.1"
  },
  "keywords": [
    "react",
    "typescript",
    "vite",
    "components",
    "dialog",
    "button",
    "toast",
    "tailwindcss"
  ]
}